{
    "name": "mouf/mvc.graphqlite",
    "description": "A binding package between thecodingmachine/graphqlite and the Mouf framework",
    "license": "MIT",
    "authors": [
        {
            "name": "David NÃ©grier",
            "email": "d.negrier@thecodingmachine.com"
        }
    ],
    "keywords" : [
        "ORM",
        "mouf",
        "graphql"
    ],
    "type" : "mouf-library",
    "require": {
        "php": ">=7.2",
        "thecodingmachine/graphqlite": "^4.0",
        "mouf/mouf": "^2.0",
        "mouf/security.userservice": "^2 | ^3",
        "mouf/security.rightsservice": "^2"
    },
    "autoload": {
        "psr-4": {
            "Mouf\\GraphQLite\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11"
    },
    "scripts": {
        "phpstan": "phpstan analyse src -c phpstan.neon --level=7 --no-progress -vvv"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
